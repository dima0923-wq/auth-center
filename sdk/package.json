{
  "name": "@auth-center/sdk",
  "version": "1.0.0",
  "description": "Auth Center SDK â€” token validation and permission middleware for all platform projects",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./express": {
      "import": "./dist/esm/middleware-express.js",
      "require": "./dist/cjs/middleware-express.js",
      "types": "./dist/esm/middleware-express.d.ts"
    },
    "./next": {
      "import": "./dist/esm/middleware-next.js",
      "require": "./dist/cjs/middleware-next.js",
      "types": "./dist/esm/middleware-next.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "python"
  ],
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "peerDependencies": {
    "next": ">=14"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
